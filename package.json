{
  "name": "wishlist",
  "displayName": "Wishlist",
  "version": "0.0.1",
  "description": "Add to your wishlist",
  "author": "romerocs@protonmail.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "0.7.2",
    "@plasmohq/storage": "1.15.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@supabase/supabase-js": "2.39.3",
    "plasmo": "0.90.5",
    "radix-ui": "^1.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.10.2",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
